@page
@model ThisIsSnackis.Pages.Admin.Headers.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Rubrik</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Header[0].TheHeader)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Header) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TheHeader)
            </td>
            <td>
                    <a asp-page="./Edit" asp-route-id="@item.Id" class="custom-link">Redigera rubrik</a> 
              
            </td>
        </tr>
}



    </tbody>
</table>

<div>
    <a href="javascript:history.go(-1);" class="custom-link">Tillbaka</a>
</div>